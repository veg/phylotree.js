{
  "name": "phylotree",
  "version": "1.0.0-alpha.14",
  "main": "build/phylotree.js",
  "keywords": [
    "phylogenetics",
    "phylogeny",
    "d3",
    "branching",
    "tree"
  ],
  "ignore": [
    "index.html"
  ],
  "bin": {
    "slatkin-maddison": "./src/cli/slatkin-maddison.js"
  },
  "dependencies": {
    "bootstrap": "4",
    "commander": "^2.19.0",
    "d3": "^5.7.0",
    "d3-hierarchy": "^1.1.8",
    "d3-line": "^0.0.1",
    "d3-time-format": "^2.1.3",
    "fast-csv" : "2.x",
    "font-awesome": "4.x",
    "jquery": "3",
    "lodash": "^4.17.11",
    "pretty-data": "^0.40.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "seedrandom": "^2.4.4",
    "underscore": "1.x",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "concurrently": "^3.6.1",
    "eslint": "^5.3.0",
    "http-server": "^0.11.1",
    "jsdoc": "^3.5.5",
    "jsdom": "9.12.x",
    "mocha": "^5.2.0",
    "prettier": "^1.14.2",
    "rollup": "^0.65.2",
    "rollup-plugin-copy": "^0.2.3",
    "rollup-plugin-terser": "^2.0.2",
    "rollup-watch": "^4.3.1",
    "should": "^11.2.1",
    "source-map-support": "^0.5.9",
    "tape": "4",
    "uglify-js": "2",
    "yarn": "^1.9.4"
  },
  "scripts": {
    "docs": "sphinx-build -b html docs documentation",
    "lint": "eslint --ext .js --ignore-pattern data,docs,images,node_modules ./",
    "dev": "rm -rf build && mkdir build && rollup -m -c -w -f umd -o build/phylotree.js -- src/index.js",
    "build": "rm -rf build && mkdir build && rollup -m -c -f umd -o build/phylotree.js -- src/index.js",
    "watch": "tape-watch -r source-map-support/register test/*-test.js",
    "pretest": "rm -rf build && mkdir build && rollup -c -f umd -o build/phylotree.js -- src/index.js",
    "test": "tape -r source-map-support/register 'test/*-test.js'",
    "postpublish": "zip -j build/phylotree.js.zip -- LICENSE README.md build/phylotree.js build/phylotree.min.js build/phylotree.css"
  },
  "files": [
    "build/phylotree.css",
    "build/phylotree.js",
    "build/phylotree.min.js",
    "src/cli"
  ]
}
